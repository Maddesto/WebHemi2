{
  "name": "gixx/webhemi2",
  "description": "A Zend Framework 2 based blog engine.",
  "version": "2.0.2",
  "type": "project",
  "keywords": ["blog", "engine", "cms"],
  "homepage": "http://www.gixx-web.com",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Gabor Ivan",
      "email": "gixx@gixx-web.com",
      "homepage": "http://www.gixx-web.com",
      "role": "Developer"
    }
  ],
  "config": {
    "vendor-dir": "wh_application/vendor"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.zendframework.com/"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ext-mcrypt": "*",
    "zendframework/zend-authentication": "2.*",
    "zendframework/zend-config": "2.*",
    "zendframework/zend-console": "2.*",
    "zendframework/zend-crypt": "2.*",
    "zendframework/zend-db": "2.*",
    "zendframework/zend-http": "2.*",
    "zendframework/zend-i18n": "2.*",
    "zendframework/zend-log": "2.*",
    "zendframework/zend-modulemanager": "2.*",
    "zendframework/zend-mvc": "2.*",
    "zendframework/zend-permissions-acl": "2.*",
    "zendframework/zend-session": "2.*",
    "zendframework/zend-serializer": "2.*",
    "zendframework/zend-view": "2.*",
    "zendframework/zend-cache": "2.*"
  },
  "require-dev": {
    "phpmd/phpmd": "@stable",
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload-dev": {
    "files": ["wh_application/library/dump.php"]
  }
}
